{
    "name": "@efteling/react-native-fast-image",
    "version": "9.0.4",
    "description": "FastImage, performant React Native image component.",
    "keywords": [
        "cache",
        "cached",
        "fastimage",
        "image",
        "priority"
    ],
    "homepage": "https://github.com/efteling/react-native-fast-image#readme",
    "bugs": {
        "url": "https://github.com/efteling/react-native-fast-image/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/efteling/react-native-fast-image"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/efteling"
    },
    "license": "(MIT AND Apache-2.0)",
    "author": "Tom Bakker <tom.bakker92@gmail.com>",
    "main": "dist/index.cjs.js",
    "module": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "android",
        "!android/build",
        "ios",
        "!ios/build",
        "dist",
        "RNFastImage.podspec"
    ],
    "scripts": {
        "build": "dv-scripts build && cp src/index.js.flow dist/index.js.flow && cp src/index.js.flow dist/index.cjs.js.flow",
        "lint": "dv-scripts lint",
        "release": "dv-scripts release",
        "test": "dv-scripts test"
    },
    "prettier": {
        "semi": false,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "all"
    },
    "eslintConfig": {
        "extends": "dv-scripts"
    },
    "jest": {
        "coveragePathIgnorePatterns": [
            "ReactNativeFastImageExample*",
            "ReactNativeFastImageExampleServer*"
        ],
        "modulePathIgnorePatterns": [
            "ReactNativeFastImageExample*",
            "ReactNativeFastImageExampleServer*"
        ],
        "preset": "react-native"
    },
    "resolutions": {
        "@jest/create-cache-key-function": "^27"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/runtime": "^7.14.6",
        "@react-native/babel-preset": "^0.73.21",
        "@rnx-kit/align-deps": "^2.3.3",
        "@types/jest": "^26.0.24",
        "@types/react": "^17.0.14",
        "@types/react-native": "^0.69.5",
        "@types/react-test-renderer": "^17.0.1",
        "dv-scripts": "^1.6.0",
        "eslint-config-dv-scripts": "^1.1.1",
        "metro-react-native-babel-preset": "^0.73.7",
        "prettier": "^2.3.2",
        "react": "18.2.0",
        "react-native": "^0.71.0",
        "react-test-renderer": "18.2.0",
        "typescript": "5.1.6"
    },
    "peerDependencies": {
        "react": "16.9.0 || 16.11.0 || 16.13.1 || 17.0.1 || 17.0.2 || 18.0.0 || 18.1.0 || 18.2.0",
        "react-native": "^0.61.5 || ^0.62.3 || ^0.63.2 || ^0.64.2 || ^0.65.0 || ^0.66.0 || ^0.67.0 || ^0.68.0 || ^0.69.0 || ^0.70.0 || ^0.71.0 || ^0.72.0 || ^0.73.0"
    },
    "rnx-kit": {
        "kitType": "library",
        "alignDeps": {
            "requirements": {
                "development": [
                    "react-native@0.71"
                ],
                "production": [
                    "react-native@>=0.60"
                ]
            },
            "capabilities": [
                "babel-preset-react-native",
                "core",
                "core-android",
                "core-ios",
                "react",
                "react-test-renderer"
            ]
        }
    }
}
